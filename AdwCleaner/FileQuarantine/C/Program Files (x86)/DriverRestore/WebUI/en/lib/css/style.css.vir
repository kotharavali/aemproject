/* = Content
---------------------------------------------------- */

article {
    width: 602px;
    height: 527px;
    margin-left: 0px;
    padding: 12px 22px 6px 28px;
    background-image: url(../images/bg/content.png);
    overflow: hidden;
    z-index: 10;
}

    article p {
        margin-bottom: 5px;
    }

    article ol, article ul {
        padding-left: 20px;
        margin: 5px 0;
    }

    article li {
        margin-bottom: 2px;
    }

    article img {
        max-width: 100%;
        height: auto;
    }
    /* = Home
---------------------------------------------------- */

    article.home .overview {
        margin: 0 -4px 2px;
        background: url(../images/home/overview/bg.png) no-repeat;
        height: 118px;
        padding: 10px 0 0;
    }

        article.home .overview .top, article.home .overview .bottom {
            background-image: url(../images/home/overview/borders.png);
            height: 12px;
            z-index: 5;
        }

        article.home .overview .bottom {
            background-position: 0 -13px;
        }

        article.home .overview .content {
            padding: 2px 25px;
            z-index: 10;
        }

            article.home .overview .content .notice {
                width: 270px;
                padding-top: 0px;
                float: left;
            }

                article.home .overview .content .notice span {
                    float: left;
                    min-height: 100px;
                    margin: 0 10px 0 0;
                }

header .liveHelp {
    color: #1284bf;
    float: right;
    font-size: 14px;
    font-weight: normal;
    width: 320px;
    margin: 18px 200px 0 0;
    line-height: 20px;
    text-align: center;
    text-transform: uppercase;
}

.liveno {
    font-size: 18px;
    font-weight: bold;
}

