/* = Document Setup
---------------------------------------------------- */

* {
    margin: 0;
    padding: 0;
    position: relative;
    outline: none;
    /*border:none;
	background:none;*/
}

body {
    font-size: 11px;
    line-height: 15px;
    font-family: Arial, sans-serif;
    font-weight: normal;
    color: #525c69;
}

h1, h2, h3 {
    font-weight: bold;
    margin-bottom: 3px;
    color: #171a1e;
}

h1 {
    font-size: 14px;
    line-height: 20px;
    color: #171a1e;
    font-family: Arial, Helvetica, sans-serif;
}

h2 {
    font-size: 16px;
    line-height: 18px;
}

h3 {
    font-size: 13px;
    line-height: 17px;
}

a, a:hover, a:active, a:focus {
    text-decoration: none;
    color: #525c69;
    outline: 0px !important;
}
    *:focus{
    outline: 0 !important; }


input[type="text"] {
    height: 16px;
    padding: 5px 6px;
    margin-right: 4px;
    line-height: 16px;
    font-size: 14px;
}

input[type="checkbox"] {
    margin: -2px 5px 0 0;
    vertical-align: middle;
    background: none;
}

textarea {
    resize: none;
}

table {
    border-spacing: 0;
    *border-collapse: collapse;
}

.normalfont {
    font-weight: normal !important;
}

.bold-fnt {
    font-weight: bold !important;
}

.font18 {
    font-size: 18px !important;
}

.bg-none {
    background: none !important;
}

.mg15 {
    margin: 15px 0 0 0px !important;
}

.fr {
    float: right !important;
}

.fl {
    float: left !important;
}

.mg0 {
    margin: 0px !important;
}

.mgtop30 {
    margin: 30px 0 0 0px !important;
}

.pd3 {
    padding: 3px 0 !important;
}

.pdlft12 {
    padding: 0 0 0 12px !important;
}

.mrglft10 {
    margin: 0 0 0 10px !important;
}

.mgr5 {
    margin-right: 5px !important;
}

/* = Core Classes
---------------------------------------------------- */

/* alignment */

.left {
    float: left;
}

.right {
    float: right;
}

.textleft {
    text-align: left;
}

.textcenter {
    text-align: center;
    margin-right: auto;
    margin-left: auto;
}

.textright {
    text-align: right;
}
/* margin, padding */

.nomargin {
    margin: 0 !important;
}

.indent_40 {
    padding-left: 40px;
}

.indent_60 {
    padding-left: 60px;
}

.indent_80 {
    padding-left: 80px;
}

.indent_100 {
    padding-left: 100px;
}
