/* disabled javascript */

.noscript {
    width: 100%;
    height: 30px;
    padding: 35px;
    text-align: center;
    background-image: url(../images/noscript.png);
    background-repeat: repeat;
    position: fixed;
    z-index: 200;
    clear: both;
    right: 0;
    left: 0;
    top: 0;
}

    .noscript div {
        width: 800px;
        margin: 0 auto;
        font-size: 20px;
        line-height: 30px;
        color: #f00;
    }
/* miscellaneous */

.clear {
    clear: both;
    font-size: 1px;
    height: 0;
}

.hline {
    clear: both;
    border: none;
    border-bottom: 1px solid #d6d6d6;
    margin: 5px 0 8px;
    font-size: 1px;
    height: 0;
}
