/* = Page
---------------------------------------------------- */

#page {
    width: 800px;
    height: 630px;
    background-image: url(../images/bg/page.jpg);
    overflow: hidden;
    margin: 0 auto;
}
/* = Header
---------------------------------------------------- */

header {
    height: 78px;
}

    header .logo {
        width: 299px;
        height: 61px;
        overflow: hidden;
        background-image: url(../images/header/logo.png);
        position: absolute;
        display: block;
        left: 17px;
        top: 22px;
        background-repeat: no-repeat;
    }

