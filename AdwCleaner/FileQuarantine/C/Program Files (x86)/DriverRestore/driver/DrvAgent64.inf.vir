; INF file for TVICHW64 Installation under Windows XP 64-bit
; Copyright 1997-2004, Victor Ishikeev, e-mail: tools@entechtaiwan.com
; Right-click this file from the Windows Explorer then select "Install"

[version]
signature="$CHICAGO$"
CatalogFile=DrvAgent64.cat

[DefaultInstall]
CopyFiles=TVICHW32.Copy.Inf, TVICHW32.Copy.Drv
AddReg=TVICHW32Reg
Uninstall=TVICHW32Remove

[SoftwareInstall]
CopyFiles=TVICHW32.Copy.Inf, TVICHW32.Copy.Drv
AddReg=TVICHW32Reg
Uninstall=TVICHW32Remove

[TVICHW32Remove]
DelFiles=driveragent.DelFiles.Inf
DelFiles=TVICHW32.Copy.Inf, TVICHW32.Copy.Drv
DelReg=TVICHW32Reg.Remove

[DestinationDirs]
driveragent.DelFiles.Inf=17
TVICHW32.Copy.Inf=17
TVICHW32.Copy.Drv=12

[TVICHW32.Copy.Drv]
DrvAgent64.sys

[TVICHW32.Copy.Inf]
DrvAgent64.inf

[driveragent.DelFiles.Inf]
driveragent_np.Inf
driveragent_np.PNF

[SourceDisksNames]
98="DriverAgent Direct I/O Driver (64-bit)",,

[SourceDisksFiles]
DrvAgent64.sys=98
DrvAgent64.inf=98


[TVICHW32Reg]
HKLM,"System\CurrentControlSet\Services\DrvAgent64","ErrorControl",%REG_DWORD%,0x00000001
HKLM,"System\CurrentControlSet\Services\DrvAgent64","Type",%REG_DWORD%,0x00000001
HKLM,"System\CurrentControlSet\Services\DrvAgent64","Start",%REG_DWORD%,0x00000002
HKLM,"System\CurrentControlSet\Services\DrvAgent64","Group",%REG_SZ%,"Extended Base"
HKLM,"System\CurrentControlSet\Services\DrvAgent64","Parameters",%REG_SZ%,""

[TVICHW32Reg.Remove]
HKLM,"System\CurrentControlSet\Services\DrvAgent64"

[Strings]
REG_SZ         = 0x00000000
REG_MULTI_SZ   = 0x00010000
REG_EXPAND_SZ  = 0x00020000
REG_BINARY     = 0x00000001
REG_DWORD      = 0x00010001
